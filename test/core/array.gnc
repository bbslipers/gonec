Функция МассивСвойства(М)
    Сообщить("Длина:    " + Строка(Длина(М)))
    Сообщить("Значение: " + Строка(М))
КонецФункции

М1 = []
МассивСвойства(М1)

М2 = Новый Массив
МассивСвойства(М2)

М3 = Диапазон(0)
МассивСвойства(М3)


М1 = М1 + ["Первый элемент"]
М1 = М1 + ["Второй элемент"]
Сообщить(М1.Найти("Второй элемент"))
Сообщить(М1.Найти("Третий элемент"))
МассивСвойства(М1)


М1.Сортировать()
МассивСвойства(М1)

М1.ОбратныйПорядок()
МассивСвойства(М1)

М1.СлучайныйПорядок()
МассивСвойства(М1)

М2 = М1.Копировать()
МассивСвойства(М2)

М3 = [1,2,3,4,5,1,2,3,7,8,9]
М3 = М3.КопироватьУникальные()
МассивСвойства(М3)

М3.Добавить(1111)
МассивСвойства(М3)

М3.СлучайныйПорядок()
МассивСвойства(М3)